{
  "name": "safire",
  "version": "0.0.1",
  "description": "TypeScript discord.js bot utilizing the Sapphire framework.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --cache --report-unused-disable-directives --ext .js,.ts --ignore-path .gitignore",
    "test": "jest",
    "build": "tsc --version && tsc",
    "commitz": "cz",
    "doc": "typedoc src/index.ts --out docs/typedoc/",
    "pm2": "pm2 start ecosystem.config.yml",
    "pm2stop": "pm2 stop ecosystem.config.yml",
    "start": "node -r dotenv/config ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/BrynAlt/safire.git"
  },
  "keywords": [
    "discord",
    "discordjs",
    "sapphire",
    "typescript"
  ],
  "author": "Bryn",
  "license": "MIT or Creative Commons Zero v1.0 Universal",
  "bugs": {
    "url": "https://gitlab.com/BrynAlt/safire/issues"
  },
  "homepage": "https://gitlab.com/BrynAlt/safire#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/prompt": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.0",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-gitlab": "^2.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-new-with-error": "^2.0.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-regexp": "^0.12.1",
    "eslint-plugin-sonarjs": "^0.8.0-125",
    "eslint-plugin-ternary": "^1.0.4",
    "eslint-plugin-total-functions": "^4.7.2",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "eslint-plugin-unicorn": "^34.0.0",
    "eslint-plugin-write-good-comments": "^0.1.3",
    "eslint-plugin-yml": "^0.9.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-junit": "^12.0.0",
    "pm2": "^5.1.0",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@discordjs/voice": "^0.5.1",
    "@sapphire/decorators": "^2.1.4",
    "@sapphire/framework": "^2.0.0-next.6bb30794.0",
    "discord-api-types": "^0.19.0-next.f393ba520d7d6d2aacaca7b3ca5d355fab614f6e",
    "discord.js": "github:discordjs/discord.js",
    "dotenv": "^10.0.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "winston-transport": "^4.4.0"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
