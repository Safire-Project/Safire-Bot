{
  "name": "safire",
  "version": "0.0.1",
  "description": "TypeScript discord.js bot utilizing the Sapphire framework.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --cache --report-unused-disable-directives --ext .js,.ts --ignore-path .gitignore",
    "test": "jest",
    "build": "tsc --version && tsc",
    "buildWithoutChecks": "tsc --version && tsc --skipLibcheck",
    "commitz": "cz",
    "doc": "typedoc src/index.ts --out docs/typedoc/",
    "pm2": "pm2 start ecosystem.config.yml",
    "pm2stop": "pm2 stop ecosystem.config.yml",
    "start": "node -r dotenv/config ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/BrynAlt/safire.git"
  },
  "keywords": [
    "discord",
    "discordjs",
    "sapphire",
    "typescript"
  ],
  "author": "Bryn",
  "license": "MIT or Creative Commons Zero v1.0 Universal",
  "bugs": {
    "url": "https://gitlab.com/BrynAlt/safire/issues"
  },
  "homepage": "https://gitlab.com/BrynAlt/safire#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/prompt": "^13.1.0",
    "@types/chart.js": "^2.9.34",
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-gitlab": "^2.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-new-with-error": "^2.0.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-regexp": "^1.1.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-ternary": "^2.0.0",
    "eslint-plugin-total-functions": "^4.10.1",
    "eslint-plugin-typescript-sort-keys": "^1.8.0",
    "eslint-plugin-unicorn": "^36.0.0",
    "eslint-plugin-write-good-comments": "^0.1.3",
    "eslint-plugin-yml": "^0.10.1",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "jest-junit": "^12.3.0",
    "pm2": "^5.1.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@discordjs/collection": "^0.2.1",
    "@discordjs/voice": "^0.6.0",
    "@sapphire/decorators": "next",
    "@sapphire/discord-utilities": "next",
    "@sapphire/discord.js-utilities": "next",
    "@sapphire/framework": "next",
    "chart.js": "^2.9.4",
    "chart.js-image": "^5.6.3",
    "common-tags": "^1.8.0",
    "discord-api-types": "^0.23.1",
    "discord-md-tags": "^1.0.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "winston-transport": "^4.4.0",
    "ws": "^8.2.2"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
